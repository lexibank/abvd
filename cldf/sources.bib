    degree = {DA},
    digital_formats = {PDF 8.22Mb image-only PDF},
    fn = {eurasia\jonsson_southwestern1991v2.pdf, eurasia\jonsson_southwestern1991.pdf, eurasia\jonsson_tai1991_o.pdf},
